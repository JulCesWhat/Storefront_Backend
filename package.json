{
    "name": "storefront_backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "db-migrate up && ENV=dev nodemon src/server.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
        "migrate-dev": "db-migrate --env dev up",
        "migrate-test": "db-migrate --env test up",
        "drop-test-db": "db-migrate db:drop store_test",
        "test": "SET ENV=test && db-migrate db:create store_test && db-migrate --env test up && ENV=test jasmine-ts && npm run drop-test-db",
        "test_OSX": "db-migrate db:create store_test && db-migrate --env test up && ENV=test jasmine-ts && npm run drop-test-db"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/express": "^4.17.9",
        "@types/pg": "^7.14.7",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.12",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.5.1",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/jasmine": "^3.8.1",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/request": "^2.48.6",
        "jasmine": "^3.8.0",
        "jasmine-spec-reporter": "^6.0.0",
        "jasmine-ts": "^0.3.0",
        "nodemon": "^2.0.12",
        "request": "^2.88.2",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9"
    }
}
